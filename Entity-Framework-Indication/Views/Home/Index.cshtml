@model List<Student>
@{
    ViewData["Title"] = "Index";
}

<br />
<h1>Home Index</h1>
<hr />
<br />

<table class="table-striped offset-1 container">
    <thead>
        <tr class="row">
            <th class="col-1 border">ID</th>
            <th class="col-2 border">Firstname</th>
            <th class="col-2 border">Secondname</th>
            <th class="col-1 border">Schoolyear</th>
            <th class="col-2 border">Phonenumber</th>
        </tr>
    </thead>
    <tbody>
    @foreach (var item in Model)
    {
        @await Html.PartialAsync("_Student", item);
    }
    </tbody>
</table>